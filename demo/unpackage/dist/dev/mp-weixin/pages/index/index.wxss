	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: left;
	-webkit-align-items: left;
	        align-items: left;
	-webkit-box-pack: left;

<view class="address-list"><navbar vue-id="1" back="{{true}}" url="{{currentTab}}" isTabPage="{{currentTab&&currentTab.length>0}}" title="家庭列表" bind:__l="__l"></navbar><block wx:for="{{address}}" wx:for-item="addr" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['selectAddress',['$0'],[[['address','id',addr.id]]]]]]]}}" class="{{['icon-list-item',(currentAddress.id==addr.id)?'selected':'']}}" bindtap="__e"><label class="icon"><image src="../../static/images/building.png"></image></label><label class="label">{{addr.familyName}}</label><block wx:if="{{currentAddress.id==addr.id}}"><label class="checked fa fa-check"></label></block></view></block><view data-event-opts="{{[['tap',[['gotoManager',['$event']]]]]}}" class="icon-list-item" bindtap="__e"><label class="icon"><image src="../../static/images/building.png"></image></label><label class="label">家庭管理</label></view></view>
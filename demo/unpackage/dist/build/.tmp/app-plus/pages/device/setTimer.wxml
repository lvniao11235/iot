<view class="set-timer"><navbar vue-id="1" back="{{true}}" title="设置定时" bind:__l="__l"></navbar><view><view class="title">定时开机<switch checked="{{selectDevice.turnOn}}" data-event-opts="{{[['change',[['switchTurnOn',['$event']]]]]}}" bindchange="__e"></switch><block wx:if="{{startTime}}"><label style="font-size:13px;float:right;color:#adadad;"><label style="display:inline-block;margin-right:3px;">{{startTime.time}}</label><label>每周<block wx:for="{{startTime.weeks}}" wx:for-item="day" wx:for-index="__i0__" wx:key="id"><label style="display:inline-block;margin-right:3px;">{{day.text}}</label></block></label></label></block></view></view><view><view class="title">定时关机<switch checked="{{selectDevice.turnOff}}" data-event-opts="{{[['change',[['switchTurnOff',['$event']]]]]}}" bindchange="__e"></switch><block wx:if="{{shutdownTime}}"><label style="font-size:13px;float:right;color:#adadad;"><label style="display:inline-block;margin-right:3px;">{{shutdownTime.time}}</label><label>每周<block wx:for="{{shutdownTime.weeks}}" wx:for-item="day" wx:for-index="__i1__" wx:key="id"><label style="display:inline-block;margin-right:3px;">{{day.text}}</label></block></label></label></block></view></view></view>